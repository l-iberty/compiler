<!-- 教材 p128 代码的语法树 -->

<程序>
	<分程序>
		<T>begin</T>
		<说明语句表>
			<说明语句表>
				<说明语句>
					<变量说明>
						<T>integer</T>
						<变量>
							<标识符>
								<字母>m</字母>
							</标识符>
						</变量>
					</变量说明>
				</说明语句>
			</说明语句表>
			<T>;</T>
			<说明语句>
				<函数说明>
					<T>integer</T><T>function</T>
					<标识符>
						<字母>F</字母>
					</标识符>
					<T>(</T>
					<参数>
						<变量>
							<标识符>
								<字母>n</字母>
							</标识符>
						</变量>
					</参数>
					<T>)</T><T>;</T>
					<函数体>
						<T>begin</T>
						<说明语句表>
							<说明语句>
								<变量说明>
									<T>integer</T>
									<变量>
										<标识符>
											<字母>n</字母>
										</标识符>
									</变量>
								</变量说明>
							</说明语句>
						</说明语句表>
						<T>;</T>
						<执行语句表>
							<条件语句>
								<T>if</T>
								<条件表达式>
									<算数表达式>
										<项>
											<因子>
												<变量>
													<标识符>
														<字母>n</字母>
													</标识符>
												</变量>
											</因子>
										</项>
									</算数表达式>
									<关系运算符>
										<T>&lt;=</T> <!-- <= -->
									</关系运算符>
									<算数表达式>
										<项>
											<因子>
												<常数>
													<无符号整数>
														<数字>0</数字>
													</无符号整数>
												</常数>
											</因子>
										</项>
									</算数表达式>
								</条件表达式>
								<T>then</T>
								<执行语句>
									<赋值语句>
										<变量>
											<标识符>
												<字母>F</字母>
											</标识符>
										</变量>
										<T>:=</T>
										<算术表达式>
											<项>
												<因子>
													<常数>
														<无符号整数>
															<数字>1</数字>
														</无符号整数>
													</常数>
												</因子>
											</项>
										</算术表达式>
									</赋值语句>
								</执行语句>
								<T>else</T>
								<执行语句>
									<赋值语句>
										<变量>
											<标识符>
												<字母>F</字母>
											</标识符>
										</变量>
										<T>:=</T>
										<算术表达式>
											<项>
												<因子>
													<变量>
														<标识符>
															<字母>n</字母>
														</标识符>
													</变量>
												</因子>
											</项>
											<T>*</T>
											<因子>
												<函数调用>
													<标识符>
														<字母>F</字母>
													</标识符>
													<T>(</T>
													<算数表达式>
														<算数表达式>
															<项>
																<因子>
																	<变量>
																		<标识符>
																			<字母>n</字母>
																		</标识符>
																	</变量>
																</因子>
															</项>
														</算数表达式>
														<T>-</T>
														<项>
															<因子>
																<常数>
																	<无符号整数>
																		<数字>1</数字>
																	</无符号整数>
																</常数>
															</因子>
														</项>
													</算数表达式>
													<T>)</T>
												</函数调用>
											</因子>
										</算术表达式>
									</赋值语句>
								</执行语句>
							</条件语句>
						</执行语句表>
						<T>end</T>
					</函数体>
				</函数说明>
			</说明语句>
		</说明语句表>
		<T>;</T>
		<执行语句表>
			<执行语句表>
				<执行语句表>
					<执行语句>
						<读语句>
							<T>read</T>
							<T>(</T>
							<变量>
								<标识符>
									<字母>m</字母>
								</标识符>
							</变量>
							<T>)</T>
						</读语句>
					</执行语句>
				</执行语句表>
				<T>;</T>
				<执行语句>
					<赋值语句>
						<变量>
							<标识符>
								<字母>m</字母>
							</标识符>
						</变量>
						<T>:=</T>
						<算数表达式>
							<项>
								<因子>
									<函数调用>
										<标识符>
											<字母>F</字母>
										</标识符>
										<T>(</T>
										<算数表达式>
											<项>
												<因子>
													<变量>
														<标识符>
															<字母>m</字母>
														</标识符>
													</变量>
												</因子>
											</项>
										</算数表达式>
										<T>)</T>
									</函数调用>
								</因子>
							</项>
						</算数表达式>
					</赋值语句>
				</执行语句>
			</执行语句表>
			<T>;</T>
			<执行语句>
				<写语句>
					<T>write</T>
					<T>(</T>
					<变量>
						<标识符>
							<字母>m</字母>
						</标识符>
					</变量>
					<T>)</T>
				</写语句>
			</执行语句>
		</执行语句表>
		<T>end</T>
	</分程序>
</程序>